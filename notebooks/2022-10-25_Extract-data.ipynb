    "import os\n",
    "\n",
    "from glob import glob\n",
    "from zipfile import ZipFile\n",
    "\n",
    "dir_data = '../data'\n",
    "\n",
    "filelist = glob(os.path.join(dir_data, '*.7z'))\n",
    "for fpath in filelist:\n",
    "    f = ZipFile(fpath)\n",
    "    f.extractall(dir_data)"
